{
	"prettier.enable": false,
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.defaultFormatter": "biomejs.biome",
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"editor.formatOnSave": true,
	"search.exclude": {
		"**/db/generated/**": true
	},
	"files.exclude": {
		"**/db/generated/**": true
	}
}
